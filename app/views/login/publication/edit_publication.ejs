<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>IDID Lab | AdminArea</title>
    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="css/style.css">
  </head>
  <body>
    <!-- Navbar -->
    <nav class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/dashboard">AdminArea</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li><a href="/dashboard">Dashboard</a></li>
            <li class="active"><a href="/view_publication">Publications</a></li>
            <li><a href="/view_team">Team</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li><a href="">Welcome user</a></li>
            <li><a href="">Logout</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <!-- Header -->
    <header id="header">
        <div class="container">
            <div class="row">
                <div class="col-md-10">
                    <h1><i class="fa fa-cog fa-spin" aria-hidden="true"></i> Publication <small>manage your publicatioins</small></h1>
                </div>

                <div class="dropdown create">
                    <div class="col-md-2">
                        <a href="/add_publication" class="btn btn-default" role="button">Add Publication</a>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Breadcrumb -->
    <section id="breadcrumb">
        <div class="container">
            <ol class="breadcrumb">
                <li><a href="/dashboard">Dashboard</a></li>
                <li class="active">Publications</li>
            </ol>
        </div>
    </section>

    <!-- Main -->
    <section id="main">
        <div class="container">
            <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    <div class="panel panel-default">
                        <div class="panel-heading main-color-bg">
                          <h3 class="panel-title">Publications</h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <input class="form-control" type="text" placeholder="Filter Pages...">
                                </div>
                            </div>
                            <br>
                            <table class="table table-striped table-hover">
                                <tr>
                                    <th>Title</th>
                                    <th>Published</th>
                                    <th>Created</th>
                                    <th>Action</th>
                                </tr>
                                <tr>
                                    <td>Home</td>
                                    <td><i class="fa fa-check-square" aria-hidden="true"></i></td>
                                    <td>Dec 12, 2016</td>
                                    <td><a href="edit.html"><i class="fa fa-pencil" aria-hidden="true"></i></a> <span id="delete"><i class="fa fa-trash" aria-hidden="true"></i></span></td>
                                </tr>
                                <tr>
                                    <td>About</td>
                                    <td><i class="fa fa-check-square" aria-hidden="true"></i></td>
                                    <td>Dec 13, 2016</td>
                                    <td><a href="edit.html"><i class="fa fa-pencil" aria-hidden="true"></i></a> <i class="fa fa-trash" aria-hidden="true"></i></td>
                                </tr>
                                <tr>
                                    <td>Services</td>
                                    <td><i class="fa fa-check-square" aria-hidden="true"></i></td>
                                    <td>Dec 13, 2016</td>
                                    <td><a href="edit.html"><i class="fa fa-pencil" aria-hidden="true"></i></a> <i class="fa fa-trash" aria-hidden="true"></i></td>
                                </tr>
                                <tr>
                                    <td>Contact</td>
                                    <td><i class="fa fa-check-square" aria-hidden="true"></i></td>
                                    <td>Dec 14, 2016</td>
                                    <td><a href="edit.html"><i class="fa fa-pencil" aria-hidden="true"></i></a> <i class="fa fa-trash" aria-hidden="true"></i></td>
                                </tr>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
  </body>
</html>
